  "components": {
    "schemas": {
      "UserRegisterDto": {
        "required": [
          "fullName",
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "fullName": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "birthday": {
            "type": "string"
          },
          "gender": {
            "$ref": "#/components/schemas/Gender"
          },
          "citizenship": {
            "type": "string"
          },
          "phoneNumber": {
            "type": "string",
            "format": "tel"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "LoginCredentialsDto": {
        "required": [
          "password",
          "email"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "EditPasswordDto": {
        "required": [
          "oldPassword",
          "newPassword",
          "email"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          },
          "oldPassword": {
            "type": "string"
          },
          "newPassword": {
            "type": "string"
          }
        }
      },
      "TokenPairDto": {
        "required": [
          "accessToken",
          "refreshToken"
        ],
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string"
          },
          "refreshToken": {
            "type": "string"
          }
        }
      },
      "ApplicantDto": {
        "required": [
          "fullName",
          "email",
          "createTime",
          "id"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "fullName": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "createTime": {
            "type": "string",
            "format": "date-time"
          },
          "birthday": {
            "type": "string"
          },
          "gender": {
            "$ref": "#/components/schemas/Gender"
          },
          "сitizenship": {
            "type": "string"
          },
          "phoneNumber": {
            "type": "string",
            "format": "tel"
          }
        }
      },
      "ApplicantEditDto": {
        "required": [
          "fullName",
          "email"
        ],
        "type": "object",
        "properties": {
          "fullName": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "birthday": {
            "type": "string"
          },
          "gender": {
            "$ref": "#/components/schemas/Gender"
          },
          "сitizenship": {
            "type": "string"
          },
          "phoneNumber": {
            "type": "string",
            "format": "tel"
          }
        }
      },
      "Gender": {
        "type": "string",
        "enum": [
          "male",
          "female"
        ]
      },
      "EducationLevelDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "minLength": 1
          }
